      DTS:ObjectName="PaymentHistoryQuery">
      <DTS:VariableValue
        DTS:DataType="8">SELECT PH.*, P.TransactionDate FROM Payment_History as PH Inner Join Payments as P ON PH.Acct_Num = P.Acct_Num WHERE (PH.Total_Amount BETWEEN 60.00 AND 80.00) AND (P.TransactionDate BETWEEN '6/7/2017' AND '8/1/2017')</DTS:VariableValue>
