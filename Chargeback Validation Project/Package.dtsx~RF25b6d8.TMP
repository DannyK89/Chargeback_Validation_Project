        public bool validateData(string Last4, string CreditType, string Amount, DateTime TransDate)
        {
            bool samelast = false;
            bool sametype = false;
            bool sameamount = false;
            bool samedate = false;
            if (Last4 == this.Last4)
                samelast = true;
            if (CardTypeCheck(CreditType).ToLower() == this.CreditType.ToLower())
                sametype = true;
            if (Amount == this.Amount)
                sameamount = true;
            if (checkDate(TransDate))
                samedate = true;
            if (samelast && sametype && sameamount && samedate)
                return true;
            else
