        private bool checkDate(DateTime TransDate)
        {
            string thisDate = "" + this.TransDate.Month + "/" + this.TransDate.Day + "/" + this.TransDate.Year;
            string sentDate = "" + TransDate.Month + "/" + this.TransDate.Day + "/" + this.TransDate.Year;
            string sentDate_1 = "" + TransDate.AddDays(-1).Month + "/" + this.TransDate.AddDays(-1).Day + "/" + this.TransDate.AddDays(-1).Year;
            string sentDate_2 = "" + TransDate.AddDays(-2).Month + "/" + this.TransDate.AddDays(-2).Day + "/" + this.TransDate.AddDays(-2).Year;
            string sentDate_3 = "" + TransDate.AddDays(-3).Month + "/" + this.TransDate.AddDays(-3).Day + "/" + this.TransDate.AddDays(-3).Year;
            string sentDate1 = "" + TransDate.AddDays(1).Month + "/" + this.TransDate.AddDays(1).Day + "/" + this.TransDate.AddDays(1).Year;
            string sentDate2 = "" + TransDate.AddDays(2).Month + "/" + this.TransDate.AddDays(2).Day + "/" + this.TransDate.AddDays(2).Year;
            string sentDate3 = "" + TransDate.AddDays(3).Month + "/" + this.TransDate.AddDays(3).Day + "/" + this.TransDate.AddDays(3).Year;
            if (sentDate == thisDate)
                return true;
            else if (sentDate1 == thisDate)
                return true;
            else if (sentDate2 == thisDate)
                return true;
            else if (sentDate3 == thisDate)
                return true;
            else if (sentDate_1 == thisDate)
                return true;
            else if (sentDate_2 == thisDate)
                return true;
            else if (sentDate_3 == thisDate)
                return true;
            else
                return false;
        }

        private string CardTypeCheck(string cType)
        {
            string CardType = "";
            if (cType == "AX")
                CardType = "American Express";
            if (cType == "DI")
                CardType = "Discover";
            if (cType == "MC")
                CardType = "Master Card";
            if (cType == "VI")
                CardType = "Visa";

            return CardType;
        }
    }

    public List<CaseRecord> CreateList(string recordList)
    {
        string[] initialList = recordList.Split(',');
        List<CaseRecord> CaseList = new List<CaseRecord>();
        foreach(string record in initialList)
        {
            CaseRecord cRecord = new CaseRecord();
            string recordString = record.Replace("{", "").Replace("}", "");
            string[] values = recordString.Split('_');
            cRecord.Last4 = values[0];
            cRecord.CreditType = values[1];
            cRecord.Amount = values[2];
            cRecord.TransDate = DateTime.ParseExact(values[3], "yyyy-MM-dd", null);
            CaseList.Add(cRecord);
        }

        return CaseList;
    }

