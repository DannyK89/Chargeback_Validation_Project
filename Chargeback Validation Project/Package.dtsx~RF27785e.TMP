    ///  Row.ZipCode = zipCode
    /// </summary>
    /// <param name="Row">The row that is currently passing through the component</param>
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        if (DateMax == null)
            DateMax = DateTime.Parse(Row.TransactionDate);

        if (DateMin == null)
            DateMin = DateTime.Parse(Row.TransactionDate).AddDays(-3);

        if (DateMax < DateTime.Parse(Row.TransactionDate))
            DateMax = DateTime.Parse(Row.TransactionDate);

        if (DateMin > DateTime.Parse(Row.TransactionDate))
            DateMin = DateTime.Parse(Row.TransactionDate).AddDays(-3);

        if (AmountMax == 0.0m)
            AmountMax = decimal.Parse(Row.TransactionAmount);

        if (AmountMin == 0.0m)
            AmountMin = decimal.Parse(Row.TransactionAmount);

        if (AmountMax < decimal.Parse(Row.TransactionAmount))
            AmountMax = decimal.Parse(Row.TransactionAmount);

        if (AmountMin > decimal.Parse(Row.TransactionAmount))
            AmountMin = decimal.Parse(Row.TransactionAmount);

        recordString += "{" +
            Row.AccountNumber.Substring(Row.AccountNumber.Length - 4, 4) + "_" +
            Row.PaymentType + "_" + Row.TransactionAmount + "_" +
            Row.TransactionDate +
            "},";

        Output0Buffer.AddRow();
        Output0Buffer.Column = recordString;
    }

}
