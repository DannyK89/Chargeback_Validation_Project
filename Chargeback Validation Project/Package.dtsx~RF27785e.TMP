    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{A2609C63-6091-4B68-947B-68FF2DF833B0}"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="ChargebackQuery">
      <DTS:VariableValue
        DTS:DataType="8">SELECT      CHAS.DownloadDate as DateReceived,  CHAS.CCType,  CHAS.ACCT_NUM as AccountNumber,  POL.POLICY_NUM as PolicyNumber,  CHAS.AmountinDispute,  CHAS.ChargeDate,  VIT.AGENT_NAME as SellerName,  POL.COMPANY_NAME as AdminName,  CON.PAYMENTS_MADE as PaymentsMade,  CON.PAYMENTS_REMAINING as PaymentsRemaining,  CHAS.RC + ' ' + chasc.clientdisplayreason as ReasonCode,  case when noticetype='' then 'Blank' when noticetype='Dispute Arbitration Decision' then 'Arbitration Decision' when noticetype='Dispute Arbitration Request' then 'Arbitration Request' when noticetype='Incoming Chargeback'                              then 'Chargeback' when noticetype='AX Inquiry Notice'                       then 'Inquiry' when noticetype='Chargeback'                                  then 'Chargeback' when noticetype='Chargeback Reversal'                then 'Chargeback Reversal' when noticetype='Dispute Ticket Retrieval Request' then 'Inquiry' when noticetype='Incoming Ticket Retrieval'       then 'Inquiry' when noticetype='Incoming Pre-Arbitration'       then 'Pre-Arbitration Request' when noticetype='Inquiry Ticket Retrieval Request' then 'Inquiry' when noticetype='Pre-Arbitration Decision'         then 'Pre-Arbitration Decision' when noticetype='Pre-Arbitration Inquiry Request' then 'Pre-Arbitration Request' when noticetype='Representment Decision'                        then 'Blank' when noticetype='Inquiry'                                           then 'Inquiry' when noticetype='Pre-Arbitration Request'          then 'Pre-Arbitration Request' when noticetype='Blank'                                                              then 'Blank' when noticetype='Arbitration Decision'                 then 'Arbitration Decision' when noticetype='Arbitration Request'                  then 'Arbitration Request' else noticetype end as NoticeType,    CON.Billing_Process as CurrentPaymentMethod, case when NAD.auto_draft_type='CREDIT' and NAD.Hold=1 then 'Y' else 'N' end as CCOnHold  FROM         CHARGEBACK_REVERSAL CHAS with(nolock)  LEFT OUTER JOIN A_ASCII_POL POL with(noexpand) ON CHAS.ACCT_NUM = POL.ACCT_NUM  LEFT OUTER JOIN A_ASCII_CON CON with(noexpand) ON CHAS.ACCT_NUM = CON.ACCT_NUM  LEFT OUTER JOIN A_ASCII_VIT VIT with(noexpand) ON CHAS.ACCT_NUM = VIT.ACCT_NUM left outer join nextautodraftdate NAD with(nolock) on CHAS.Acct_Num = NAD.Acct_num and auto_draft_type='CREDIT' left outer join chargeback_reversal_codes CHASC with(nolock) on chas.cctype=chasc.cctype and chas.rc=chasc.code  where  CHAS.DownloadDate between '8/1/2017' and '10/10/2017'</DTS:VariableValue>
    </DTS:Variable>
