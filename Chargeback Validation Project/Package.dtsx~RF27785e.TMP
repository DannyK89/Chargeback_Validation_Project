        string tempLast4 = last4.ToString();
        int offset = 0;

        if (tempLast4.Length != 4)
        {
            string zeros = "";
            offset = 4 - tempLast4.Length;
            for (int x = 0; x < offset; x++){
                zeros += "0";
            }
            newLast4 = zeros + "" + tempLast4;
        }
        else
            newLast4 = tempLast4;

        return newLast4;
    }

